<form method="post" class="d-inline" action="{{ path('app_secteur_delete', {'id': secteur.id}) }}" onsubmit="return confirm('Êtes-vous sûr de supprimer ce sécteur?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ secteur.id) }}">
<button type="submit" class="btn btn-danger btn-sm"><i class="fa fa-trash"></i></button>
</form>
